{
  "compilerOptions": {
    "target": "es2020",
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist/",
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}